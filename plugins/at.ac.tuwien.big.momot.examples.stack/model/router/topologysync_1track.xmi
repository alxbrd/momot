<?xml version="1.0" encoding="ASCII"?>
<topologysync:Container
    xmi:version="2.0"
    xmlns:xmi="http://www.omg.org/XMI"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xmlns:topologysync="http://topologysync/1.0"
    xsi:schemaLocation="http://topologysync/1.0 topologysync.ecore">
  <topology
      Name="Example1"
      evp="//@evp.0">
    <trackSegments
        xsi:type="topologysync:Track"
        Name="T1"
        length="1"
        evpTrack="//@evp.0/@evpElements.0">
      <conns
          Name="A"
          Type="1"/>
      <conns
          Name="B"
          Type="2"/>
    </trackSegments>
  </topology>
  <evp Name="Example1"
      topology="//@topology.0">
    <evpElements
        xsi:type="topologysync:EVPTrack"
        Name="T1"
        len="1"
        topTrack="//@topology.0/@trackSegments.0"/>
  </evp>
</topologysync:Container>
