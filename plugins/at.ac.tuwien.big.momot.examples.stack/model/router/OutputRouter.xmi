<router:Container xmi:version="2.0" xmlns:xmi="http://www.omg.org/XMI" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns:router="http://router/1.0">
  <agents xsi:type="router:Provider" name="pc3" speed="12" rightserv="//@devices.0"/>
  <agents xsi:type="router:Provider" name="s1" speed="12" rightserv="//@devices.1"/>
  <agents xsi:type="router:Consumer" name="pc2" speed="9" rightcomp="//@devices.5"/>
  <agents xsi:type="router:Consumer" name="pc3" speed="17" rightcomp="//@devices.4"/>
  <devices xsi:type="router:Server" name="r1" provider="//@agents.0">
    <outgoing xsi:type="router:GlassFiberCable" speed="4" target="//@devices.2"/>
  </devices>
  <devices xsi:type="router:Server" name="pc2" provider="//@agents.1">
    <outgoing xsi:type="router:GlassFiberCable" speed="4" target="//@devices.3"/>
  </devices>
  <devices xsi:type="router:Router" incoming="//@devices.0/@outgoing.0" name="pc1" maxSpeed="15">
    <outgoing xsi:type="router:CopperCable" speed="0" target="//@devices.4"/>
  </devices>
  <devices xsi:type="router:Router" incoming="//@devices.1/@outgoing.0" name="s2" maxSpeed="2">
    <outgoing xsi:type="router:CopperCable" speed="0" target="//@devices.5"/>
  </devices>
  <devices xsi:type="router:Computer" incoming="//@devices.2/@outgoing.0" name="pc3" consumer="//@agents.3"/>
  <devices xsi:type="router:Computer" incoming="//@devices.3/@outgoing.0" name="pc2" consumer="//@agents.2"/>
</router:Container>
